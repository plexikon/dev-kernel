{
    "name": "plexikon/kernel",
    "description": "shared kernel",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "steph bug",
            "email": "plexikon@protonmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "beberlei/assert": "^3.2",
        "marc-mabe/php-enum": "^4.2",
        "ext-json": "*",
        "ramsey/uuid": "^3.9",
        "react/promise": "^2.7",
        "plexikon/reporter": "dev-develop",
        "plexikon/chronicle": "dev-develop",
        "ext-pcntl": "*"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.5.3",
        "laravel/framework": "7.9.*",
        "orchestra/testbench": "^5.1",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/plexikon/reporter"
        },
        {
            "type": "vcs",
            "url": "https://github.com/plexikon/chronicle"
        }
    ],
    "autoload": {
        "psr-4": {
            "Plexikon\\Kernel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Plexikon\\Kernel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": []
        }
    }
}
